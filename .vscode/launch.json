{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            //add below to avoid no module found error
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        }
    ]
}